{
  "appId": "com.openwhen.boxofus",
  "appName": "Open When",
  "webDir": "build",
  "server": {
    "androidScheme": "https"
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": false
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#f7f4f1",
      "androidSplashResourceName": "splash",
      "showSpinner": false
    }
  }
}
